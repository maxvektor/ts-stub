{
    "env": {
        "jest/globals": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parser": "typescript-eslint-parser",
    "plugins": [
        "prettier",
        "typescript",
        "jest"
    ],
    "settings": {
        "import/resolver": {
            "typescript": {}
        }
    },
    "overrides": {
        "files": [
            "**/*.ts",
            "**/*.tsx"
        ],
        "parser": "typescript-eslint-parser",
        "rules": {
            "no-undef": "off",
            "no-unused-vars": "off"
        }
    }
}